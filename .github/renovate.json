{
  "extends": ["config:base"],
  "enabledManagers": ["gradle", "gradle-wrapper"],
  "packageRules": [
    {
      "matchPackagePatterns": [".*"],
      "enabled": false
    },
    {
      "matchPackageNames": [
        "androidx.activity:activity-compose"
      ],
      "enabled": true
    },
    {
      "matchPackagePatterns": [
        "^org\\.jetbrains\\.compose"
      ],
      "enabled": true
    }
  ]
}
